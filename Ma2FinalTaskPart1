//Main classs
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		StudentData data = new StudentData();
		
		Scanner scan = new Scanner(System.in);
		
		
		System.out.println("[Input Personal Data]");
		System.out.println("Full Name: ");
		data.setName(scan.nextLine());
		System.out.println("Phone Number: ");
		data.setPhone(scan.nextLong());
		scan.nextLine();
		System.out.println("Address: ");
		data.setAdd(scan.nextLine());
		System.out.println("Age: ");
		data.setAge(scan.nextInt());
		System.out.println("\r[Input Grade]");
		System.out.println("English Grade: ");
		data.setEng_grade(scan.nextInt());
		System.out.println("Filipino Grade: ");
		data.setFil_grade(scan.nextInt());
		System.out.println("Math Grade: ");
		data.setMath_grade(scan.nextInt());
		System.out.println("Science Grade: ");
		data.setSci_grade(scan.nextInt());
		System.out.println("MAPEH Grade: ");
		data.setMapeh_grade(scan.nextInt());
		
		
		
		System.out.println("\r-Student Data-");
	  System.out.println("Name: "+ data.getName());
		System.out.println("Contact Number: "+ data.getPhone());
		System.out.println("Address: "+ data.getAdd());
		System.out.println("Age: "+ data.getAge());
		System.out.println("\r-Student Grade-");
		System.out.println("English Grade: "+ data.getEng_grade());
		System.out.println("Filipino Grade: "+ data.getFil_grade());
		System.out.println("Math Grade: "+ data.getMath_grade());
		System.out.println("Science Grade: "+ data.getSci_grade());
		System.out.println("MAPEH Grade: "+ data.getMapeh_grade());
		
		
		scan.close();
		
	
	}
}


//Second class StudentData.java

public class StudentData {
	
	private String name, add;
	private long phone;
	private int age, eng_grade, fil_grade, math_grade, sci_grade, mapeh_grade;
	

		
		public void setName(String name) {
			this.name = name;}
		
		public void setPhone(long phone) {
			this.phone = phone;}
		
		public void setAdd(String add) {
			this.add = add;}
		
		public void setAge(int age) {
			this.age = age;}
		
		public void setEng_grade(int eng_grade) {
			this.eng_grade = eng_grade;}
		
		public void setFil_grade(int fil_grade) {
			this.fil_grade = fil_grade;}
		
		public void setMath_grade(int math_grade) {
			this.math_grade = math_grade;}
		
		public void setSci_grade(int sci_grade) {
			this.sci_grade = sci_grade;}
		
		public void setMapeh_grade(int mapeh_grade) {
			this.mapeh_grade = mapeh_grade;}
		
	
		
		public String getName() {
			return name;}
		
		public long getPhone() {
			return phone;}
		
		public String getAdd() {
			return add;}
		
		public Integer getAge() {
			return age;}
		
		public Integer getEng_grade() {
			return eng_grade;}
		
		public Integer getFil_grade() {
			return fil_grade;}
		
		public Integer getMath_grade() {
			return math_grade;}
		
		public Integer getSci_grade() {
			return sci_grade;}
		
		public Integer getMapeh_grade() {
			return mapeh_grade;
		
		}
		}



